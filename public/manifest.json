{
  "name": "VueCita",
  "manifest_version": 2,
  "version": "1.0.1",
  "minimum_chrome_version": "10.0",
  "devtools_page": "index.html",
  "permissions":[
    "tabs", 
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "vuecita_logo.png"
  }
}